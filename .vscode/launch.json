{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Listen for Xdebug",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "log": true,
      "xdebugSettings": {
        "max_children": 256,
        "max_data": 1024,
        "max_depth": 5
      },
      "pathMappings": {
        "${workspaceFolder}": "${workspaceFolder}"
      }
    }
  ]
}
